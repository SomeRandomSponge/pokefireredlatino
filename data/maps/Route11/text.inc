Route11_Text_DarianIntro::
    .string "Nunca he ganado antes…$"

Route11_Text_DarianDefeat::
    .string "Lo veía venir…$"

Route11_Text_DarianPostBattle::
    .string "Tuve mala suerte, como siempre.$"

